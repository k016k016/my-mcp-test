# Preview環境の環境変数
# Vercel Preview環境（developブランチ）用の設定

# 環境識別子
NODE_ENV=production
NEXT_PUBLIC_ENV=preview

# マルチドメイン設定（Preview環境）
# VercelのPreview環境URL（developブランチ）
# 注：実際のURLは初回デプロイ後に確認してください
NEXT_PUBLIC_WWW_URL=https://my-mcp-test-git-develop-k016k016s-projects.vercel.app
NEXT_PUBLIC_APP_URL=https://my-mcp-test-git-develop-k016k016s-projects.vercel.app
NEXT_PUBLIC_ADMIN_URL=https://my-mcp-test-git-develop-k016k016s-projects.vercel.app
NEXT_PUBLIC_OPS_URL=https://my-mcp-test-git-develop-k016k016s-projects.vercel.app

# OPS用IP制限（Preview環境でテスト）
OPS_ALLOWED_IPS=your-office-ip,your-vpn-ip

# Supabase（Preview用プロジェクト）
NEXT_PUBLIC_SUPABASE_URL=your-preview-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-preview-supabase-anon-key

# Cloudflare R2（Preview用バケット）
R2_ACCOUNT_ID=your-r2-account-id
R2_ACCESS_KEY_ID=your-preview-r2-access-key
R2_SECRET_ACCESS_KEY=your-preview-r2-secret-key
R2_BUCKET_NAME=preview-your-bucket-name
R2_PUBLIC_URL=https://preview-your-bucket.r2.dev

# Upstash Redis（Preview用インスタンス）
UPSTASH_REDIS_REST_URL=your-preview-redis-url
UPSTASH_REDIS_REST_TOKEN=your-preview-redis-token

# Sentry（Preview環境）
NEXT_PUBLIC_SENTRY_DSN=your-preview-sentry-dsn
SENTRY_ORG=your-org
SENTRY_PROJECT=your-project-preview
SENTRY_AUTH_TOKEN=your-sentry-auth-token

# Logflare（Preview用）
NEXT_PUBLIC_LOGFLARE_API_KEY=your-preview-logflare-api-key
NEXT_PUBLIC_LOGFLARE_SOURCE_ID=your-preview-logflare-source-id

# Chargebee（テストモード）
CHARGEBEE_SITE=your-chargebee-test-site
CHARGEBEE_API_KEY=your-chargebee-test-api-key

# Resend（Preview用APIキー）
RESEND_API_KEY=your-preview-resend-api-key
