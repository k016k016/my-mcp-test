{
  "name": "my-mcp-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:preview": "env-cmd -f .env.preview next dev --turbopack",
    "build": "next build",
    "build:preview": "env-cmd -f .env.preview next build",
    "build:production": "env-cmd -f .env.production next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:preview": "PLAYWRIGHT_BASE_URL=https://www.cocktailorder.com playwright test",
    "test:e2e:production": "PLAYWRIGHT_BASE_URL=https://your-production-domain.com playwright test",
    "env:dev": "cp .env.development .env.local && echo '✅ 開発環境に切り替えました'",
    "env:preview": "cp .env.preview .env.local && echo '✅ Preview環境に切り替えました'",
    "env:production": "cp .env.production .env.local && echo '✅ 本番環境に切り替えました (注意して使用してください)'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.908.0",
    "@aws-sdk/s3-request-presigner": "^3.908.0",
    "@sentry/nextjs": "^10.19.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.0",
    "@upstash/redis": "^1.35.5",
    "chargebee-typescript": "^2.53.0",
    "next": "15.5.4",
    "pino": "^10.0.0",
    "pino-logflare": "^0.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "^6.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.0",
    "@react-email/render": "^1.3.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "env-cmd": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jsdom": "^27.0.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
