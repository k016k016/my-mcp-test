# 開発環境の環境変数
# ローカル開発用の設定

# 環境識別子
NODE_ENV=development
NEXT_PUBLIC_ENV=development

# マルチドメイン設定（ローカル開発）
NEXT_PUBLIC_WWW_URL=http://localhost:3000
NEXT_PUBLIC_APP_URL=http://app.localhost:3000
NEXT_PUBLIC_ADMIN_URL=http://admin.localhost:3000
NEXT_PUBLIC_OPS_URL=http://ops.localhost:3000

# OPS用IP制限（開発環境では無効）
OPS_ALLOWED_IPS=

# Supabase（開発用プロジェクト）
NEXT_PUBLIC_SUPABASE_URL=your-dev-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-dev-supabase-anon-key

# Cloudflare R2（開発用バケット）
R2_ACCOUNT_ID=your-dev-r2-account-id
R2_ACCESS_KEY_ID=your-dev-r2-access-key
R2_SECRET_ACCESS_KEY=your-dev-r2-secret-key
R2_BUCKET_NAME=dev-your-bucket-name
R2_PUBLIC_URL=https://dev-your-bucket.r2.dev

# Upstash Redis（開発用インスタンス）
UPSTASH_REDIS_REST_URL=your-dev-redis-url
UPSTASH_REDIS_REST_TOKEN=your-dev-redis-token

# Sentry（開発環境では無効化推奨）
NEXT_PUBLIC_SENTRY_DSN=
SENTRY_ORG=
SENTRY_PROJECT=
SENTRY_AUTH_TOKEN=

# PostHog（開発用プロジェクト）
NEXT_PUBLIC_POSTHOG_KEY=your-dev-posthog-key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Chargebee（テストモード）
CHARGEBEE_SITE=your-chargebee-test-site
CHARGEBEE_API_KEY=your-chargebee-test-api-key

# Resend（開発用APIキー）
RESEND_API_KEY=your-dev-resend-api-key
